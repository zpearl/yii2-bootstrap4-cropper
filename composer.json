{
  "name": "zpearl/yii2-bootstrap4-cropper",
  "description": "Cropper for Yii2",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "cropper"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitriy Sabirov",
      "email": "web8dew@yandex.ru"
    },
    {
      "name": "zpearl",
      "email": "zpearl80@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "fxp/composer-asset-plugin": "^1.4.4",
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.6",
    "yiisoft/yii2-bootstrap4": "*",
    "npm-asset/cropperjs": "~1.4.3"
  },
  "autoload": {
    "psr-4": {
      "zpearl\\cropper\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    },
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm-asset",
      "bower-asset-library": "vendor/bower-asset"
    }
  }
}
